{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Dimppl",
  "version": "0.1.0",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "Lifestyle",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "",
    "resources": [],
    "shortDescription": "",
    "targets": "all",
    "iOS": {
      "frameworks": [
        "CoreAudio",
        "AudioToolbox"
      ]
    }
  },
  "identifier": "com.dhbtk.dimppl",
  "app": {
    "windows": [
      {
        "height": 600,
        "label": "main",
        "title": "Dimppl",
        "width": 1024
      }
    ]
  },
  "plugins": {}
}
